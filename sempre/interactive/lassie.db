##########################################################################
# Domain knowledge is written here in a mix of whitespace- or            #
# comma-separated values. For each line, the first token is a HOL        #
# component and the second is the attribute we want to specify. What     #
# follows is a comma-separated list of values which characterize this    #
# attribute of that component. In the list (and nowhere else) can tokens #
# contain whitespaces. This allows SML types and expressions of more     #
# than one word to be represented naturally.                             #
#                                                                        #
# The `type` attribute is the only one required for each component       #
# (required for forcing well-typed terms in the grammar). All attributes #
# give rise to a syntactic category of the same name (e.g. `name` gives  #
# rise to the `$name` cat) which hold values of that attribute during    #
# derivation. The final ontology covers every "relation" expressed in    #
# this file, plus a reflexive name for each component, i.e.              #
#                                                                        #
#                        REAL_ADD name REAL_ADD                          #
#                                                                        #
# Allowing users to specify components by their literal name if they     #
# know it.                                                               #
##########################################################################

#########################
# HOL4 Tactics - Types: #
#########################

# rewriting
asm_rewrite_tac             type    thm list -> tactic
rewrite_tac                 type    thm list -> tactic
once_rewrite_tac            type    thm list -> tactic
once_asm_rewrite_tac        type    thm list -> tactic

# simplification
simp                        type    thm list -> tactic
fs                          type    thm list -> tactic
rfs                         type    thm list -> tactic

# thm tactics
imp_res_tac                 type    thm -> tactic
assume_tac                  type    thm -> tactic
irule 		            type    thm -> tactic
drule                       type    thm -> tactic
match_mp_tac                type    thm -> tactic

# automation tactics
res_tac                     type    tactic
metis_tac                   type    thm -> tactic
meson_tac                   type    thm -> tactic

# pattern based tactics
qpat_x_assum                type    term quotation -> (thm -> tactic) -> tactic
qpat_assum                  type    term quotation -> (thm -> tactic) -> tactic

# tacticals
first_x_assum               type    (thm -> tactic) -> tactic
last_x_assum                type    (thm -> tactic) -> tactic
ntac 		            type    int -> tactic -> tactic
rpt 		            type    tactic -> tactic

# other
subgoal                     type    term quotation -> tactic
by                          type    term quotation * tactic -> tactic
suffices_by                 type    term quotation * tactic -> tactic
Cases                       type    tactic
Induct                      type    tactic
cheat 		            type    tactic
strip_tac                   type    tactic
Induct_on 	            type    term quotation -> tactic
rename                      type    term quotation list -> tactic
qexists_tac                 type    term quotation -> tactic
qspec_then                  type    term quotation -> (thm -> tactic) -> thm -> tactic
qspecl_then                 type    term quotation list -> (thm -> tactic) -> thm -> tactic

########################
# HOL4 - Rules : Types #
########################
GSYM                        type    thm -> thm
EVAL_RULE                   type    thm -> thm
Once                        type    thm -> thm

#################################
# HOL4 Tactics - Name features: #
#################################
# rewriting
asm_rewrite_tac             name    rewrite with assumptions
rewrite_tac                 name    rewrite
once_rewrite_tac            name    rewrite once
once_asm_rewrite_tac        name    rewrite once with assumptions, rewrite with assumptions once

# simplification
simp                        name    simplify
fs                          name    full simplify
rfs                         name    reverse full simplify

# thm tactics
imp_res_tac                 name    resolve implication
assume_tac                  name    assume, have
irule 		            name    match with, imp
drule                       name    resolve with
match_mp_tac                name    modus ponens

# automation tactics
res_tac                     name    resolve all
metis_tac                   name    metis
meson_tac                   name    meson

# pattern based tactics
qpat_x_assum                name    remove with pattern
qpat_assum                  name    find with pattern

# tacticals
first_x_assum               name    first matching assumption
last_x_assum                name    last matching assumption
ntac 	                    name    repeat exactly
rpt 	                    name    repeat infinitely, repeat

# other
subgoal                     name    show
by                          name    by
suffices_by                 name    suffices to show
Cases                       name    Case analysis, case analysis
Induct                      name    Induction, induction
cheat 		            name    cheat
strip_tac                   name    strip, remove quantifier
Induct_on 	            name    Induction on, Induct on, induction on, induct on
rename                      name    rename into
qexists_tac                 name    exists, exists_tac
qspec_then                  name    specialize for
qspecl_then                 name    specialize for all

########################
# HOL4 - Rules : Types #
########################
GSYM                        name    symmetry, gsym
EVAL_RULE                   name    eval_rule, evaluate
Once                        name    only once, once

